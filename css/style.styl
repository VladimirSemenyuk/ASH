.nav
  min-width: 1024px;
  list-style: none;
  padding: 0;
  margin: 0;
  text-align: center;
  z-index: 200;
  padding 0;

.nav_item
  padding: 0;
  margin: 10px 20px;
  display: inline-block;
  font-family: 'Open Sans', sans-serif;
  font-size: 14px;
  letter-spacing: 0px;
  border-bottom: 1px dotted;

  .small-nav &
    font-size: 12px;
    color: #666
    border-color: #888
    margin-bottom 20px;

    & a
      color: #444444
      text-decoration none
      text-transform capitalize

  &_current
    border-bottom: 1px solid;

  & a
    color: #222
    text-decoration none
    text-transform uppercase

    &:hover
      color: #000



.slider
  position relative
  padding 0
  margin 0 auto 80px
  list-style none
  width: 1000px
  height 450px
  box-shadow 0 1px 2px rgba(0,0,0,0.4)

  & ul
    width: 1000px
    height 450px
    padding 0
    margin 0
    list-style none

  &__prev
    position: absolute;
    display: inline-block;
    width: 50px;
    height: 50px;
    top: 50%;
    margin-top: -25px;
    left: 0;
    text-align: center;
    line-height: 43px;
    cursor: pointer;
    opacity: 0.5;
    font-size: 60px;
    color: white;
    text-shadow 1px 1px 1px #333, -1px 1px 1px #333, 1px -1px 1px #333, -1px -1px 1px #333;

    &:hover
      opacity 1

  &__dots
    text-align center
    margin 10px auto

  &__dot
    width: 10px
    height 10px
    border-radius 10px
    border 1px solid #333333
    box-shadow 0 1px 1px rgba(0,0,0,0.3)
    display inline-block
    margin 10px
    cursor pointer

    &.active
      background-color #333333

  &__next
    position: absolute;
    display: inline-block;
    width: 50px;
    height: 50px;
    top: 50%;
    margin-top: -25px;
    right: 0;
    text-align: center;
    line-height: 43px;
    cursor: pointer;
    opacity: 0.5;
    font-size: 60px;
    color: white;
    text-shadow 1px 1px 1px #333, -1px 1px 1px #333, 1px -1px 1px #333, -1px -1px 1px #333;

    &:hover
      opacity 1

.model-scroller
  overflow-x auto
  width 1200px
  margin 0 auto

  &.mCS_no_scrollbar
    width 1200px


.model-list
  list-style none
  padding 0
  text-align center
  margin 10px 0 0

  &_lined
    display: flex

  &__item
    display inline-block
    width: 200px
    margin 5px 20px 5px 20px
    cursor pointer
    font-family: 'Open Sans', sans-serif;
    font-size: 14px;
    vertical-align top

    &__image
      background-position: 50% 100% !important;
      width: 200px
      height: 580px
      background-repeat: no-repeat;

      .model-list_max-scale-24-75 &
        height: (550+0)px

      .model-list_max-scale-25-5 &
        height: (573+0)px

      .model-list_max-scale-33 &
        height: (641+0)px

      .model-list_max-scale-34 &
        height: (641+0)px

      .model-list_max-scale-35 &
        height: (664+0)px

      .model-list_max-scale-36 &
        height: (664+0)px

      &:hover
        margin-top -10px;
        margin-bottom 10px;

    &__name
      text-transform capitalize

    &__info
      color: crimson



.slide
  border none
  width: 1000px
  height 450px
  background-position-y 15%
  background-size cover
  opacity 0;
  position absolute
  top: 0;
  left 0;
  list-style none

.model
  &__info-text
    font-size: 16px
    font-style italic
    text-align center
    color: #555555
    margin 150px 0

  &__description
    font-size: 16px
